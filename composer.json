{
    "name": "aerospike-community/aerospike-client-rest-php",
    "description": "REST Interface for Aerospike Database.",
    "keywords": [
        "openapitools",
        "openapi-generator",
        "openapi",
        "php",
        "sdk",
        "rest",
        "api"
    ],
    "homepage": "http://www.aerospike.com",
    "license": "Apache2.0",
    "authors": [
        {
            "email": "info@aerospike.com",
            "name": "Joe Martin",
            "homepage": "https://docs.aerospike.com",
            "role": "maintainer"
        },
        {
            "name": "OpenAPI-Generator contributors",
            "homepage": "https://openapi-generator.tech"
        }
    ],
    "require": {
        "php": ">=7.2",
        "ext-curl": "*",
        "ext-json": "*",
        "ext-mbstring": "*",
        "guzzlehttp/guzzle": "^6.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.0 || ^9.0",
        "friendsofphp/php-cs-fixer": "^2.12"
    },
    "autoload": {
        "psr-4": { "OpenAPI\\Client\\" : "lib/" }
    },
    "autoload-dev": {
        "psr-4": { "OpenAPI\\Client\\Test\\" : "test/" }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
